{
  "name": "kietvetran@github.io",
  "version": "1.0.0",
  "description": "Kietvetran webpage",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "serve": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js && cross-env serve dist/",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{ts,tsx}' --check",
    "prettier:fix": "npm run prettier -- --write",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "pre-commit": "tsc --noEmit && npm run lint -- --fix"
  },
  "dependencies": {
